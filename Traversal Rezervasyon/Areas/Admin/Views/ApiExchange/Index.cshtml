@model IEnumerable<BookingExchangeViewModel.Exchange_Rates>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminUILayout.cshtml";
}
<div class="col-lg-12 grid-margin stretch-card">
	<div class="card">
		<div class="card-body">
			<h4 class="card-title">Kur Oranı Listesi</h4>

			<div class="table-responsive">
				<table class="table table-striped">
					<thead>
						<tr>
							<th>Kur Adı</th>
							<th>Değeri</th>
						</tr>
					</thead>
					<tbody>
						@foreach (var item in Model)
						{
							<tr>
								<td>@item.currency</td>
								<td>@item.exchange_rate_buy</td>
							</tr>
						}

					</tbody>
				</table>
			</div>
			<br />

		</div>
	</div>
</div>
