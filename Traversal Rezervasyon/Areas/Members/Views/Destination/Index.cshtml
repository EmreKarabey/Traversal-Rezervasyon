@using EntityLayer.Concrete

@model List<Destination>
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/Shared/_MemberUILayout.cshtml";
}
<h1>Aktif Tur Rotası Tablosu</h1>

<table class="table table-bordered">

	<tr>
		<th>#</th>
		<th>Şehir</th>
		<th>Açıklama</th>
		<th>Fiyat</th>
		<th>Kapasite</th>
		<th>Sil</th>
		<th>Güncelle</th>
	
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>@item.DestinationID</td>
			<td>@item.City</td>
			<td>@item.Description</td>
			<td>@item.Price</td>
			<td>@item.Capacity</td>
			<td><a class="btn btn-info" href="#">Sil</a></td>
			<td><a class="btn btn-warning" href="#">Güncelle</a></td>
	
		</tr>
	}



</table>

